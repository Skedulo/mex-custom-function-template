{
  "name": "{{ cookiecutter.project_slug }}",
  "version": "1.0.0",
  "description": "{{ cookiecutter.project_name }}",
  "main": "index.js",
  "license": "UNLICENSED",
  "owner": "skedulo",
  "devDependencies": {
    "husky": "^8.0.2",
    "workspaces": "^0.1.0"
  },
  "scripts": {
    "dev": "yarn app start",
    "clean": "yarn libs clean && yarn app clean",
    "bundle": "yarn app bundle",
    "package": "rm -f customfunction.tar.gz && tar -zcvf customfunction.tar.gz --exclude={\".git\",\"node_modules\",\"dist\",\"build\",\"coverage\"} .",
    "app": "yarn workspace custom-function",
    "libs": "yarn workspace mex-custom-function-lib",
    "submodule-update": "git submodule update --recursive"
  },
  "workspaces": [
    "custom-function",
    "mex-custom-function-lib"
  ],
  "private": true,
  "packageManager": "yarn@3.1.1"
}
